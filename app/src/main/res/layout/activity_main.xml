<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.tanilsoo.tambet_telvis.qrreader.MainActivity"
    android:weightSum="1">

    <TextView
        android:text="Sisselogitud: "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/employee_logged"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="LADU"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lao_scan"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/prindi_qr_activity_btn"
        android:layout_toStartOf="@+id/laadimine_scan"
        android:layout_weight="0.25" />

    <Button
        android:text="Immutus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/immutus_scan"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_weight="0.25" />

    <Button
        android:text="Laadimine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/laadimine_scan"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_weight="0.25" />

    <Button
        android:text="PRINDI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/prindi_qr_activity_btn"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="changeToPrintLayout"
        android:layout_weight="0.25" />



</TableLayout>
